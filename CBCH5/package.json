  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:archive": "ch5-cli archive -p CB -c ../Contractor/output/CBlink/interface/mapping/CBlink.cse2j -d dist/CB -o dist",
    "build:deploy": "ch5-cli deploy -H 192.168.1.70 -t touchscreen dist/CB.ch5z",
    "build:deploy1060": "ch5-cli deploy -H 192.168.1.101 -t touchscreen dist/CB.ch5z",
    "build:onestep": "yarn build:prod && yarn build:archive && yarn build:deploy",
    "build:onestep1060": "yarn build:prod && yarn build:archive && yarn build:deploy1060",
    "build:deployweb": "ch5-cli deploy -H 192.168.1.2 -p -t web dist/CB.ch5z",
    "build:onestepweb": "yarn build:prod && yarn build:archive",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
